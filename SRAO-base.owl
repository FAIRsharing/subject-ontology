    <!-- http://www.fairsharing.org/ontology/subject/SRAO_0000169 -->

    <owl:Class rdf:about="http://www.fairsharing.org/ontology/subject/SRAO_0000169">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NCIT_C16421"/>
        <rdfs:subClassOf rdf:resource="http://www.fairsharing.org/ontology/subject/SRAO_0000236"/>
        <obo:IAO_0000115 xml:lang="en">Solid-State Chemistry, also sometimes referred as materials chemistry, is the study of the synthesis, structure, and properties of solid phase materials, particularly, but not necessarily exclusively of, non-molecular solids.</obo:IAO_0000115>
        <obo:IAO_0000117 xml:lang="en">Allyson Lister, https://orcid.org/0000-0002-7702-4495</obo:IAO_0000117>
