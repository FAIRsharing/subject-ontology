    </owl:Class>
    


    <!-- http://www.fairsharing.org/ontology/subject/SRAO_0000144 -->

    <owl:Class rdf:about="http://www.fairsharing.org/ontology/subject/SRAO_0000144">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NCIT_C95096"/>
        <obo:IAO_0000115 xml:lang="en">Biological Psychiatry is the branch of psychiatry concerned with the biological basis for mental disorders, typically using physical methods or medication (as opposed to psychotherapy) as treatment.</obo:IAO_0000115>
        <obo:IAO_0000117 xml:lang="en">Allyson Lister, https://orcid.org/0000-0002-7702-4495</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">OED, AL 18.6.2019</obo:IAO_0000119>
        <obo:IAO_0000412 xml:lang="en">re3data</obo:IAO_0000412>
        <rdfs:label xml:lang="en">Biological Psychiatry</rdfs:label>
    </owl:Class>
    


