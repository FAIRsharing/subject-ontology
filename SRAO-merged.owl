        <obo:IAO_0000412 rdf:resource="http://edamontology.org/EDAM.owl"/>
    </owl:Class>
    


    <!-- http://edamontology.org/topic_3056 -->

    <owl:Class rdf:about="http://edamontology.org/topic_3056">
